{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "Run Example",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}",
			],
			"outFiles": [
				"${workspaceRoot}/dist/*.js"
			],
			"sourceMaps": true,
			"preLaunchTask": "npm: build"
		}
	]
}